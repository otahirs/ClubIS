@inject NavigationManager NavigationManager
@inject IdentityAuthenticationStateProvider AuthStateProvider
<MudNavMenu>
    <MudNavLink Href="" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Home">Home</MudNavLink>
    <MudNavLink Href="/events" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.DirectionsRun">Event Entries</MudNavLink>
    <MudNavLink Href="/users" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.People">Member List</MudNavLink>
    <MudNavLink Href="/finance" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.AccountBalanceWallet">Finance</MudNavLink>
    <MudNavLink Href="/settings" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Settings">Settings</MudNavLink>
    <AuthorizeView Policy="ShowAdminMenu">
        <MudDivider DividerType="DividerType.Middle" Class="my-8"/>
        <MudText Typo="Typo.h6" Align="Align.Center" Class="my-3">Administration</MudText>
    </AuthorizeView>
    <AuthorizeView Policy="@Policy.News">
        <MudNavLink Href="/admin/news" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Message">News</MudNavLink>
    </AuthorizeView>
    <AuthorizeView Policy="@Policy.Entries">
        <MudNavLink Href="/admin/events" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.SportsKabaddi">Events</MudNavLink>
    </AuthorizeView>
    <AuthorizeView Policy="@Policy.Users">
        <MudNavLink Href="/admin/users" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Groups">Users</MudNavLink>
    </AuthorizeView>
    <AuthorizeView Policy="@Policy.Entries">
        <MudNavLink Href="/admin/entries" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.StickyNote2">Entries Export</MudNavLink>
    </AuthorizeView>
    <AuthorizeView Policy="@Policy.Finance">
        <MudNavLink Href="/admin/finance" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.AccountBalance">Finance Managment</MudNavLink>
    </AuthorizeView>
</MudNavMenu>